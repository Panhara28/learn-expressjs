{
  "name": "express-restapi",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/knex": "^0.16.1",
    "@types/node": "^18.16.2",
    "dot-env": "^0.0.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "knex": "^2.4.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.2.4",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts --exec ts-node",
    "knex migration:make": "ts-node ./node_modules/knex/bin/cli.js migrate:make --knexfile=./path/to/knexfile.ts",
    "knex migrate:latest": "ts-node ./node_modules/knex/bin/cli.js migrate:latest --knexfile=./path/to/knexfile.ts",
    "test": "tsc && mocha --require ts-node/register src/**/*.spec.ts --exit"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/chalk": "^2.2.0",
    "@types/mocha": "^10.0.1",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.7",
    "chalk": "^5.2.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3"
  }
}
